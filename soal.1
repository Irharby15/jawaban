<html>
<title>jawaban no.1</title>
<body>
<script>
var Nilai = prompt("Masukan Nilai")
        if (Nilai > 90) {
            document.write(Nilai + ":" +
                "grade A")
        } else if (Nilai > 80) {
            document.write(Nilai + ":" +
                "grade B")
        } else if (Nilai > 70) {
            document.write(Nilai + ":" + "grade C")
        } else {
            document.write(Nilai + ":" + "grade F")
        }
        </script>
</body>
</html>
