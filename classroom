<html>

<body>
    <script>
        var Nama = prompt("MASUKAN NAMA MAHASISWA:")
        var Nilai = prompt("MASUKAN NILAI SISWA")

        if (Nilai > 70) {
            document.writeln("SELAMAT ANDA TELAH LULUS" + "<br>Nama:" + Nama + "" + "<br>Nilai:" + Nilai + "")
        } else {
            document.writeln("Opps! anda Tidak lulus" + "<br>Nama:" + Nama + "" + "<br>Nilai:" + Nilai + "")
        }
    </script>
</body>

</html>
